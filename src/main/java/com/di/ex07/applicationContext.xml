<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="developer" class="com.di.ex07.Developer">
		<constructor-arg value="개발자"/>
		<constructor-arg value="100000"/>
		<property name="dept" value="개발팀"/>
	</bean>

	<bean id="engineer" class="com.di.ex07.Engineer">
		<constructor-arg value="기술자"/>
		<constructor-arg value="200000"/>
		<property name="dept" value="기술팀"/>
	</bean>
	
</beans>
